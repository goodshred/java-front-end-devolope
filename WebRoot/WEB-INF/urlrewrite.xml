<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 3.0//EN" "http://tuckey.org/res/dtds/urlrewrite3.0.dtd">

<urlrewrite>   
    <rule>
        <from>^/do/(\w+)/(\w+)/(.+)$</from>
        <to last="true">/$1.do?method=$2&amp;$3</to>
    </rule>
    <rule>
        <from>^/do/(\w+)/(\w+)$</from>
        <to last="true">/$1.do?method=$2</to>
    </rule>
    <rule>
        <from>^/(\w+)$</from>
        <to last="true">/$1.do</to>
    </rule>


 

</urlrewrite>
